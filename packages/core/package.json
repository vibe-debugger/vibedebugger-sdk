{
    "name": "@vibedebugger/core",
    "version": "0.1.0",
    "description": "Core utilities for Vibe Debugger SDK",
    "main": "./dist/index.js",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "require": "./dist/index.js",
            "import": "./dist/index.mjs",
            "types": "./dist/index.d.ts"
        }
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "tsup src/index.ts --format cjs,esm --dts",
        "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
        "test": "vitest run"
    },
    "keywords": [
        "error-tracking",
        "monitoring",
        "core"
    ],
    "author": "Your Name",
    "license": "MIT",
    "publishConfig": {
        "access": "public"
    },
    "devDependencies": {
        "tsup": "^8.0.1",
        "typescript": "^5.3.3",
        "vitest": "^1.2.0"
    }
}